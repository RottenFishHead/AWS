[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/RottenFishHead
ExecStart=/home/ubuntu/RottenFishHead/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind 0.0.0.0:80 \
          RottenFishHead.wsgi:application
[Install]
WantedBy=multi-user.target
